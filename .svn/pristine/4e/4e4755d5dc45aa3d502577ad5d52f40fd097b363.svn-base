package uml.per.reg;

import com.job5156.core.common.CommonEnum;

/**
 * <p></p>
 * Date:2015/5/16 9:00
 *
 * @author pzm
 * @version 1.0
 */
public abstract class RegisterBo {
    /**
     * 请求用户的ip
     */
    private String ip;
    /**
     * 来源url域名
     */
    private String fdomain;
    /**
     * 来源url
     */
    private String fromUrl;
    /**
     * 目标url
     */
    private String targetUrl;
    /**
     * 推广参数
     */
    private String spreadParam;
    /**
     * sem推广参数
     */
    private String semSpreadParam;
    /**
     * 分站点id，这个接口中应该是首页对应的id
     */
    private Integer siteId;

    /**
     * 注册的来源 CommonEnum.ComeFrom
     */
    private CommonEnum.ComeFrom comeFrom;
    /**
     * 注册校验地址服务器
     */
    private String serverName;

    public String getIp() {
        return ip;
    }

    public void setIp(String ip) {
        this.ip = ip;
    }

    public String getFromUrl() {
        return fromUrl;
    }

    public void setFromUrl(String fromUrl) {
        this.fromUrl = fromUrl;
    }

    public String getSpreadParam() {
        return spreadParam;
    }

    public void setSpreadParam(String spreadParam) {
        this.spreadParam = spreadParam;
    }

    public String getTargetUrl() {
        return targetUrl;
    }

    public void setTargetUrl(String targetUrl) {
        this.targetUrl = targetUrl;
    }

    public Integer getSiteId() {
        return siteId;
    }

    public void setSiteId(Integer siteId) {
        this.siteId = siteId;
    }

    public String getFdomain() {
        return fdomain;
    }

    public void setFdomain(String fdomain) {
        this.fdomain = fdomain;
    }

    public CommonEnum.ComeFrom getComeFrom() {
        return comeFrom;
    }

    public void setComeFrom(CommonEnum.ComeFrom comeFrom) {
        this.comeFrom = comeFrom;
    }

    public String getServerName() {
        return serverName;
    }

    public void setServerName(String serverName) {
        this.serverName = serverName;
    }

    public String getSemSpreadParam() {
        return semSpreadParam;
    }

    public void setSemSpreadParam(String semSpreadParam) {
        this.semSpreadParam = semSpreadParam;
    }
}

