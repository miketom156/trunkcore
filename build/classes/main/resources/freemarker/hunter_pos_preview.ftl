<#--智通猎头职位详情模板-->
<table cellspacing="0" cellpadding="2" width="100%" border="0">
	<tbody>
		<tr>
			<td>
				<table width="100%" border="0" cellspacing="0" cellpadding="0">
					<tbody>
						<tr>
							<td height="40" align="center"><font class="right_1">   招聘职位：${posName}&nbsp;
								<#if (urgent)?exists && urgent == 1>
									<img src="http://images.job5156.com/images/hot.gif" title="急聘">&nbsp;
								</#if>
								(<#if recruitmentNumber == 0>不限<#else>${recruitmentNumber}</#if>人)
								</font>
							</td> 
						</tr> 
						<tr>
							<td height="1" background="http://images.job5156.com/process/hunter5156/images/job_bg.gif"></td> 
						</tr> 
						<tr>
							<td height="10"></td> 
						</tr> 
					</tbody>
				</table>
			</td>
		</tr>
		<tr>
			<td>
				<table cellspacing="0" cellpadding="0" width="98%" align="right" border="0">
					<tbody>
						<tr>
							<td>
								<table cellspacing="1" cellpadding="1" width="100%" border="0">
									<tbody>
										<tr>
											<td width="79">招聘期限：</td>
											<td colspan="3">${endDate?string('yyyy-MM-dd')}</td>
										</tr>
										<tr>
											<td>联 系 人：</td>
											<td width="168">${contactPerson}</td>
											<td valign="top" align="left" width="67">工作地区：</td>
											<td valign="top" align="left" width="147">${locationListStr} </td>
										</tr>
										<tr>
											<td>电&nbsp;&nbsp;&nbsp;&nbsp;话：</td>
											<td colspan="3">${contactPhone}</td>
										</tr>
										<tr>
											<td>电子邮件：</td>
											<td colspan="3">${emailStr}</td>
										</tr>
										<tr>
											<td>联系地址：</td>
											<td colspan="3">${addressVo?string}</td>
										</tr>
										<tr>
											<td>面试地址：</td>
											<td colspan="3">${interviewAddressVo?string}</td>
										</tr>
										<tr>
											<td>面试须知：</td>
											<td colspan="3">${interviewNote}</td>
										</tr>
									</tbody>
								</table>
							</td>
						</tr>
						<tr>
							<td colspan="3" height="10">
								<table cellspacing="0" cellpadding="0" width="100%" border="0">
									<tbody>
										<tr>
											<td bgcolor="#cccccc" colspan="4" height="1"></td>
										</tr>
										<tr>
											<td colspan="4" height="2"></td>
										</tr>
										<tr>
											<td colspan="4">
												<table cellspacing="1" cellpadding="0" width="100%" border="0">
													<tbody>
														<tr align="middle" bgcolor="#efefef">
															<td width="15%" height="20">学历</td>
															<td width="15%">工作经验</td>
															<td width="15%">年龄</td>
															<td width="10%">性别</td>
															<td width="15%">月薪</td>
															<td width="20%">要求现所在地</td>
														</tr>
													</tbody>
												</table>
											</td>
										</tr>
										<tr>
											<td colspan="4" height="2"></td>
										</tr>
										<tr>
											<td bgcolor="#cccccc" colspan="4" height="1"></td>
										</tr>
										<tr>
											<td colspan="4" height="5"></td>
										</tr>
										<tr> 
											<td colspan="4">
												<table cellspacing="0" cellpadding="1" width="100%" border="0">
													<tbody>
														<tr align="middle">
															<td width="15%">${degree}</td>
															<td width="15%">${reqWorkYearStr}</td>
															<td width="15%">
																<#if reqAgeMin != 0 && reqAgeMax != 0>
																	${reqAgeMin}~${reqAgeMax}
																<#else>
																	不限
																</#if>
															</td>
															<td width="10%">${reqGenderStr}</td>
															<td width="15%">${salaryStr}</td>
															<td width="20%">${reqLocationStr}</td>
														</tr>
													</tbody>
												</table>
											</td>
										</tr>
										<tr>
											<td colspan="4" height="5"></td>
										</tr>
										<tr>
											<td colspan="4"><font class="right_1">面试要求：</font></td>
										</tr>
										<tr>
											<td colspan="4">${description}</td>
										</tr>
									</tbody>
								</table>
							</td>
						</tr>
						<tr>
							<td height="5"></td>
						</tr>
						<tr>
							<td align="center">
								<img src="http://images.job5156.com/images/button01.gif" title="发送应聘意向" style="cursor:hand" onclick="hunterApply();" value="发送应聘意向" name="Submit2">&nbsp;&nbsp;
								<!--img src="http://images.job5156.com/images/button02.gif" title="放入收藏夹" style="cursor:hand" onclick="collectionJobs('44331','4801732')" value="放入收藏夹" name="Submit2">&nbsp;&nbsp;&nbsp;-->
								<!--img src="http://images.job5156.com/images/button03.gif" title="推荐给好友" style="cursor:hand" onclick="commendToFriend('44331','4801732')" name="Submit2">&nbsp;&nbsp;&nbsp;-->
								<!--input type="button" value="关闭窗口"  onclick="window.close()"  name="Submit2"-->
							</td>
						</tr>
						<tr>
							<td height="20"></td>
						</tr>
					</tbody>
				</table>
			</td>
		</tr>
	</tbody>
</table>